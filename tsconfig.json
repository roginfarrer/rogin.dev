{
  "extends": "astro/tsconfigs/strict",
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "components/*": ["./src/components/*"],
      "layouts/*": ["./src/layouts/*"],
      "styled-system/*": ["./styled-system/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}

